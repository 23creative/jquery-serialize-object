{
  "name": "form-serializer",
  "version": "2.0.1",
  "description": "serialize form fields into an object or JSON",
  "scripts": {
    "build": "npm run-script browserify && npm run-script minify",
    "browserify": "{ node build/header.js; browserify lib/plugin.js; } > jquery.serialize-object.js",
    "test": "open ./test/test.html",
    "minify": "uglifyjs jquery.serialize-object.js -m -c --comments > jquery.serialize-object.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macek/jquery-serialize-object"
  },
  "keywords": [
    "form",
    "serialize",
    "json",
    "object"
  ],
  "plugin": {
    "type": "jQuery",
    "name": "jQuery serializeObject"
  },
  "author": "macek <paulmacek@gmail.com>",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/macek/jquery-serialize-object/issues"
  },
  "devDependencies": {
    "jquery-extend": "~2.0.3",
    "mocha": "^1.17.1",
    "browserify": "^3.32.1",
    "uglify-js": "^2.4.12"
  }
}
